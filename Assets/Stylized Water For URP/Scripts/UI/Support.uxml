            <ui:Button name="Review" style="margin-left: 3px; padding-left: 6px; padding-top: 10px; padding-bottom: 10px; margin-bottom: 4px; margin-top: 4px;">
                <ui:VisualElement name="Button Content" style="flex-direction: row; align-items: center;">
