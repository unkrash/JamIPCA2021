<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">
    <ui:Template name="Toolbar" src="Toolbar.uxml" />
    <ui:Instance template="Toolbar" name="Toolbar" />
    <ui:VisualElement name="Support Content" focusable="false" style="opacity: 1; background-color: rgba(75, 75, 75, 0); -unity-background-image-tint-color: rgb(255, 255, 255); flex-direction: row; justify-content: center; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-top: 20px; padding-bottom: 20px; color: rgb(0, 0, 0); align-items: flex-start; flex-grow: 0; flex-shrink: 1; -unity-text-align: upper-left;">
        <Style src="Support.uss" />
        <ui:VisualElement name="Column 1" style="align-items: stretch; justify-content: flex-start; padding-left: 20px; padding-right: 10px;">
            <ui:Button name="Manual" class="support-section-button" style="margin-left: 3px; padding-left: 6px; padding-top: 10px; padding-bottom: 10px; margin-bottom: 4px; margin-top: 4px;">
                <ui:VisualElement name="Button Content" style="flex-direction: row; align-items: center;">
                    <ui:VisualElement name="Icon" class="support-section-button-icon button-icon" style="background-image: resource('Icons/Manual');" />
                    <ui:VisualElement name="Text">
                        <ui:Label text="Manual" name="Title" class="button-title" />
