        <ui:VisualElement name="Column 1" style="align-items: stretch; justify-content: flex-start; padding-left: 10px; padding-right: 20px;">
            <ui:Button name="Contact" style="margin-left: 3px; padding-left: 6px; padding-top: 10px; padding-bottom: 10px; margin-bottom: 4px; margin-top: 4px;">
                <ui:VisualElement name="Button Content" style="flex-direction: row; align-items: center;">
                    <ui:VisualElement name="Icon" class="support-section-button-icon button-icon" style="background-image: resource('Icons/Contact');" />
