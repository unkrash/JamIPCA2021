                    </ui:VisualElement>
                </ui:VisualElement>
            </ui:Button>
            <ui:Button name="Website" class="support-section-button" style="margin-left: 3px; padding-left: 6px; padding-top: 10px; padding-bottom: 10px; margin-bottom: 4px; margin-top: 4px;">
                <ui:VisualElement name="Button Content" style="flex-direction: row; align-items: center;">
                    <ui:VisualElement name="Icon" class="support-section-button-icon button-icon" style="background-image: resource('Icons/Website');" />
